# Creative Muse AI - React Frontend Environment Variables
# Copy this file to .env and configure your settings

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=10000

# App Configuration
VITE_APP_NAME=Creative Muse AI
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-powered creative idea generation platform

# Development Settings
VITE_DEV_MODE=true
VITE_DEBUG_MODE=false

# PWA Configuration
VITE_PWA_ENABLED=true
VITE_PWA_CACHE_NAME=creative-muse-cache

# Analytics (optional)
VITE_ANALYTICS_ID=your_analytics_id_here

# Feature Flags
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_MULTILINGUAL=true

# Performance Settings
VITE_LAZY_LOADING=true
VITE_IMAGE_OPTIMIZATION=true